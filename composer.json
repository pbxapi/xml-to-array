{
    "name": "pbxapi/xml-to-array",
    "type": "library",
    "description": "Easily convert valid xml to a php array.",
    "keywords": [
        "pbxapi",
        "xml",
        "convert"
    ],
    "homepage": "https://github.com/pbxapi/xml-to-array",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Townsend",
            "email": "mtownsend5512@gmail.com",
            "role": "Developer"
        }
    ],
	"autoload": {
        "psr-4": {
            "Pbxapi\\XmlToArray\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "~7.0|^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Pbxapi\\XmlToArray\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
